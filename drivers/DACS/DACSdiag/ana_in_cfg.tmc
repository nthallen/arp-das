%{
  void ana_in_cfg_init(void) {
    unsigned short addr;
    unsigned short row, col;
    for ( addr = 0xC00; addr < 0xD00; addr += 2 ) {
      sbwr( addr, 0x14 ); // Start with unity gain
    }
    for (row = 8; row <= 10; row++) {
      for ( col = 0; col < 8; col++ ) {
	addr = 0xC10 + row*0x20 + col*2;
	sbwr( addr, 0x14 );
      }
    }
    sbwr( 0xC1E, 0x100 );
    sbwr( 0xC5E, 0x120 );
    sbwr( 0xC9E, 0x140 );
  }
%}
TM INITFUNC ana_in_cfg_init();
