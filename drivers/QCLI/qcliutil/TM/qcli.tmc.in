TM "Receive" QCLI@SUFFIX@ 1;

TM 1 Hz QCLI_Wave_t QCLI@SUFFIX@_Wave;
TM 1 Hz UINT QCLI@SUFFIX@_s;
TM 1 Hz UINT QCLI@SUFFIX@_Stale;

Group QCLI@SUFFIX@_grp ( QCLI@SUFFIX@_Wave, QCLI@SUFFIX@_s ) {
  QCLI@SUFFIX@_Wave = QCLI@SUFFIX@.qcli_wave;
  QCLI@SUFFIX@_s = QCLI@SUFFIX@.status ^ 0x1000; /* flip the laser bit */
  QCLI@SUFFIX@_Stale = QCLI@SUFFIX@_obj->stale();
  QCLI@SUFFIX@_obj->synch();
}
TM 1 HZ QCLI_CS_t QCLI@SUFFIX@_CS; Address QCLI@SUFFIX@_CS @ADDR@;

TM "Receive" SSP@SUFFIX@ 1;

TM @SSPRATE@ Hz QCLI_WORD SSP@SUFFIX@_Num;
TM @SSPRATE@ Hz QCLI_WORD SSP@SUFFIX@_SN;
TM @SSPRATE@ Hz QCLI_WORD SSP@SUFFIX@_TS;
TM @SSPRATE@ Hz XLONG SSP@SUFFIX@_Flags;
TM @SSPRATE@ Hz SSP_Status_t SSP@SUFFIX@_Status;
TM @SSPRATE@ Hz UINT SSP@SUFFIX@_Stale;
TM @SSPRATE@ Hz T_FPGA_t SSP@SUFFIX@_T_FPGA;
TM @SSPRATE@ Hz T_HtSink_t SSP@SUFFIX@_T_HtSink;

Group SSP@SUFFIX@_grp ( SSP@SUFFIX@_Num, SSP@SUFFIX@_SN, SSP@SUFFIX@_TS, SSP@SUFFIX@_Flags,
		 SSP@SUFFIX@_T_FPGA, SSP@SUFFIX@_T_HtSink ) {
  SSP@SUFFIX@_Num = SSP@SUFFIX@.index;
  SSP@SUFFIX@_SN = SSP@SUFFIX@.ScanNum;
  SSP@SUFFIX@_TS = SSP@SUFFIX@.Total_Skip;
  SSP@SUFFIX@_Flags = SSP@SUFFIX@.Flags;
  SSP@SUFFIX@_Status = SSP@SUFFIX@.Status;
  SSP@SUFFIX@_T_FPGA = SSP@SUFFIX@.T_FPGA & 0xFFF8;
  SSP@SUFFIX@_T_HtSink = SSP@SUFFIX@.T_HtSink & 0xFFE0;
  SSP@SUFFIX@_Stale = SSP@SUFFIX@_obj->stale();
  SSP@SUFFIX@_obj->synch();
}
