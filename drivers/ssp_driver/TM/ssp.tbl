{ HBox{ -; "Tilde Prototype"; - };
  HBox{ +|+; { [QCLI1_Exec]; [QCLI1_Status]; [SSP1] }; |+;
              { [QCLI2_Exec]; [QCLI2_Status]; [SSP2] }; |+;
              { [QCLI3_Exec]; [QCLI3_Status]; [SSP3] }; +|+ };
  -;
  HBox{ +|+; HGlue 0+1; { MFCtr: (MFCtr,5) }; +|+ };
  -;
}
