QCLI1_Exec { # Executive Summary
  HBox { +-; Title: "QCLI1"; -+ };
  Wave: (QCLI1_Wave,10);
  Mode: { (QCLI1_mode,7) (QCLI1_laser,3); };
  Stale: (QCLI1_Stale,5);
  SSP1 { (SSP1_Num,5) (SSP1_Status,7); }
}
QCLI1_Status {
  HBox { +-; Title: "QCLI1 Status"; -+ };
  HBox {
    {
      Ready:         (QCLI1_ready,3);
      Busy:          (QCLI1_busy,3);
      Flash:         (QCLI1_flash,2);
      Cksum:         (QCLI1_cksum,1);
    }; |; {
      "QCLI T:"      (QCLI1_dot,4);
      "Laser T:"     (QCLI1_lot,4);
      "Laser I:"     (QCLI1_loc,4);
      "Inv Cmd:"     (QCLI1_cordte,4);
      "SelWave:"     (QCLI1_waveerr,4);
      "Cmd Err:"     (QCLI1_cmderr,4);
    }
  }
}

SSP1 {
  HBox { +-; Title: "SSP 1"; -+ };
  File: (SSP1_Num,5);
  Scan: (SSP1_SN,8);
  Skip: (SSP1_TS,8);
  Status: (SSP1_Status,7);
  ""  >"1|2|3 ";
  AD: (SSP1_AD,5);
  PA: (SSP1_PA,5);
  CA: (SSP1_CA,5);
  Stale: (SSP1_Stale,5);
}

